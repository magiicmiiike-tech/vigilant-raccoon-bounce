#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@_bb2434a8e627aa879e936adc09d8fe0f/node_modules/ts-jest/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@_bb2434a8e627aa879e936adc09d8fe0f/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@_bb2434a8e627aa879e936adc09d8fe0f/node_modules/ts-jest/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-jest@29.4.6_@babel+core@_bb2434a8e627aa879e936adc09d8fe0f/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../ts-jest/cli.js" "$@"
fi
