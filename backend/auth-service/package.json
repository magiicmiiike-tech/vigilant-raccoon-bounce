    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
