#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.19.27_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.19.27_typescript@5.7.3/node_modules/ts-node/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.19.27_typescript@5.7.3/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.19.27_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.19.27_typescript@5.7.3/node_modules/ts-node/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/ts-node@10.9.2_@types+node@20.19.27_typescript@5.7.3/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/ts-node@10.9.2_@types+node@20.19.27_typescript@5.7.3/node_modules/ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/ts-node@10.9.2_@types+node@20.19.27_typescript@5.7.3/node_modules/ts-node/dist/bin-transpile.js" "$@"
fi
