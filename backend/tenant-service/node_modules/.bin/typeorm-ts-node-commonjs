#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/typeorm@0.3.28_babel-plugin_1cfd899a01f88afe0b949b2bb48a4dff/node_modules/typeorm/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/typeorm@0.3.28_babel-plugin_1cfd899a01f88afe0b949b2bb48a4dff/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/typeorm@0.3.28_babel-plugin_1cfd899a01f88afe0b949b2bb48a4dff/node_modules/typeorm/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/typeorm@0.3.28_babel-plugin_1cfd899a01f88afe0b949b2bb48a4dff/node_modules:/mnt/c/Users/Pooya/OneDrive/Desktop/ai-voiceagentic/SaaS/vigilant-raccoon-bounce/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-commonjs.js" "$@"
fi
